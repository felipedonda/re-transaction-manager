@{
    ViewData["Title"] = "Home Page";
}

<div name="GeoJsonToKml">
    <h2>Converter GeoJson para KML</h2>
    <p> Faça upload de um arquivo no formato GeoJson para converter para o formato Kml.</p>

    <form method="post" enctype="multipart/form-data" asp-controller="Home" asp-action="GeojsonToKmlAction">
        <div class="form-group">
            <input type="file" name="files" single accept=".geojson,.json"/>
        </div>
        <div class="form-group">
            <input type="submit" value="Converter" />
        </div>
    </form>
</div>
    <br>
<div name="GeoJsonToShape">
    <h2>Converter GeoJson para Shape</h2>
    <p> Faça upload de um arquivo no formato GeoJson para converter para o formato Shape.</p>

    <form method="post" enctype="multipart/form-data" asp-controller="Home" asp-action="GeojsonToShapeAction">
        <div class="form-group">
            <input type="file" name="files" single accept=".geojson,.json"/>
        </div>
        <div class="form-group">
            <input type="submit" value="Converter" />
        </div>
    </form>
</div>

<p>@ViewData["Message"]</p>